    "url": "https://github.com/ponyesteves/rxjs-couchdb/issues"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ]
  },
  "homepage": "https://github.com/ponyesteves/rxjs-couchdb#readme",
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/rx": "^4.1.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^20.0.4",
    "np": "^2.16.0"
  },
  "dependencies": {
    "ramda": "^0.24.1",
