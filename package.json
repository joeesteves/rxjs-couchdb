{
  "name": "rxjs-couchdb",
  "version": "1.4.0",
  "description": "Simple rsxj implementation for couchDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "build": "babel src -d dist",
    "dev": "babel src --watch -d dist",
